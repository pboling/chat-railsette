<table>
  <thead>
    <tr>
      <th>method</th>
      <th>time</th>
      <th>args</th>
      <th>message</th>
      <th>backtrace</th>
    </tr>
  </thead>
  <tbody>
    <%- chats.each do |mc| %>
      <tr>
        <td>
          <%= mc.method_call %>
        </td>
        <td>
          <%= mc.display_time %>
        </td>
        <td>
          <%= mc.args %>
        </td>
        <td>
          <%= mc.message %>
        </td>
        <td>
          <%- mc.filtered_backtrace.each do |line| %>
            <p>
              <%= line.html_safe %>
            </p>
          <%- end %>
        </td>
      </tr>
    <%- end %>
  </tbody>
</table>
